{
  "datasets": [
    {
      "name": "52890130",
      "displayName": "fact_sales",
      "queryLines": [
        "SELECT * FROM workspace.gold.fact_sales"
      ]
    },
    {
      "name": "35aad311",
      "displayName": "Sales by Country",
      "queryLines": [
        "SELECT c.country, SUM(f.sales_amount) AS total_sales\n",
        "FROM workspace.gold.fact_sales f\n",
        "LEFT JOIN workspace.gold.dim_customers c\n",
        "  ON f.gold_customer_sk = c.gold_customer_sk\n",
        "GROUP BY c.country"
      ],
      "catalog": "workspace",
      "schema": "gold"
    },
    {
      "name": "e47daa87",
      "displayName": "Sales by Marital Status",
      "queryLines": [
        "SELECT c.marital_status, SUM(f.sales_amount) AS total_sales\n",
        "FROM workspace.gold.fact_sales f\n",
        "LEFT JOIN workspace.gold.dim_customers c\n",
        "  ON f.gold_customer_sk = c.gold_customer_sk\n",
        "GROUP BY c.marital_status"
      ],
      "catalog": "workspace",
      "schema": "gold"
    },
    {
      "name": "18819526",
      "displayName": "Top 5 Countries by Total Sales",
      "queryLines": [
        "SELECT c.country, SUM(f.sales_amount) AS total_sales\n",
        "FROM workspace.gold.fact_sales f\n",
        "LEFT JOIN workspace.gold.dim_customers c\n",
        "  ON f.gold_customer_sk = c.gold_customer_sk\n",
        "GROUP BY c.country\n",
        "ORDER BY total_sales DESC\n",
        "LIMIT 5"
      ],
      "catalog": "workspace",
      "schema": "gold"
    }
  ],
